<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <!--
        Distinct.exe returns only unique lines of text from input.
        Copyright (C) 2023 Timothy J. Bruce
    -->
  <PropertyGroup>
	  <RootNamespace>Icod.Distinct</RootNamespace>
	  <AssemblyName>Distinct</AssemblyName>
	  <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Icod.Argh" Version="1.0.1" />
  </ItemGroup>
  <ItemGroup>
    <None Update="Icod.Distinct.License">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
